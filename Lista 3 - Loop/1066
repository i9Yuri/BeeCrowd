a = float(input())
b = float(input())
c = float(input())
d = float(input())
e = float(input())

numeros = [a, b, c, d, e]
numeros_pares = 0
numeros_impares = 0
numeros_neg = 0
numeros_pos = 0

for numero in numeros:
    if numero % 2 == 0:
        numeros_pares += 1

print(f'{numeros_pares} valor(es) par(es)')

for numero in numeros:
    if numero % 2 > 0:
        numeros_impares += 1
        
print(f'{numeros_impares} valor(es) impar(es)')

for numero in numeros:
    if numero > 0:
        numeros_pos += 1
        
print(f'{numeros_pos} valor(es) positivo(s)')

for numero in numeros:
    if numero < 0:
        numeros_neg += 1
        
print(f'{numeros_neg} valor(es) negativo(s)')
